<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework-message-queue:etc/consumer.xsd">
    <!-- This defines our consumer, which listens for messages on the topic -->
    <consumer name="couture.catalogue.sync.consumer"
              queue="couture.catalogue.sync.queue"
              connection="amqp"
              handler="CoutureSearch\SearchAPI\Model\Queue\Consumer::process"/>
    
    <consumer name="couture.order.sync.consumer"
              queue="couture.order.sync.queue"
              connection="amqp"
              handler="CoutureSearch\SearchAPI\Model\Queue\OrderConsumer::process"/>

</config>
